{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "declaration": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": [
            "es5",
            "es2015"
        ],
        "module": "es2015",
        "moduleResolution": "Node",
        "noImplicitAny": true,
        "removeComments": true,
        "sourceMap": true,
        "strict": true,
        "target": "ES5"
    }
}
